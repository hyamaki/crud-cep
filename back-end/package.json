{
  "name": "teste-quality-entregas",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node ./src/server.ts\"",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node ./dist/server.js",
    "migration:create": "typeorm migration:create ./src/infrastructure/database/migrations/$npm_config_name",
    "migrate": "ts-node node_modules/typeorm/cli.js migration:run -d ./src/infrastructure/database/datasource.ts",
    "migration:down": "ts-node node_modules/typeorm/cli.js migration:revert -d ./src/infrastructure/database/datasource.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^22.10.7",
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.1.14",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.20",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/reflect-metadata": "^0.0.5",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typeorm-extension": "^3.6.3",
    "typescript": "^5.7.3"
  }
